{
  "name": "@unchainedshop/engine",
  "description": "Unchained Engine",
  "version": "1.1.0",
  "license": "EUPL-1.2",
  "homepage": "https://unchained.shop",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unchainedshop/unchained.git"
  },
  "contributors": [
    "Vedran Rudelj <vedran@unchained.shop> (https://unchained.shop)",
    "Pascal Kaufmann <pascal@unchained.shop> (https://unchained.shop)",
    "Marco Wettstein <maw@panter.ch>",
    "Simon Emanuel Schmid <simon@unchained.shop> (https://unchained.shop)",
    "Mikael Araya Mengistu <mikael@unchainewd.shop> (https://unchained.shop)"
  ],
  "bugs": {
    "url": "https://github.com/unchainedshop/unchained/issues"
  },
  "scripts": {
    "lint": "eslint packages tests --fix --ext=js --ext=ts",
    "pretest": "npm run lint --silent",
    "lint:ci": "eslint packages tests --ext=jsx --ext=js",
    "test": "jest --runInBand --detectOpenHandles --forceExit tests/*",
    "test:ci": "npm run test:integration && npm run test:packages",
    "test:integration": "jest --ci --runInBand --detectOpenHandles --verbose --forceExit tests/*",
    "test:packages": "jest --ci --runInBand --detectOpenHandles --passWithNoTests --verbose --forceExit packages/*",
    "test:watch": "cross-env DEBUG=jest-mongodb:* jest --runInBand --watch --no-watchman --verbose tests/**",
    "dev": "run-p --print-label dev:*",
    "dev:minimal": "cd examples/minimal && npm run dev",
    "dev:controlpanel": "cd examples/controlpanel && npm run dev",
    "install": "run-p --print-label install:*",
    "install:minimal": "cd examples/minimal && npm install",
    "install:controlpanel": "cd examples/controlpanel && npm install",
    "install:types": "cd packages/@unchainedshop/types && npm install && npm link",
    "postinstall": "npm run link-types",
    "link-types": "run-p --print-label link-types:*",
    "link-types:root": "npm link @unchainedshop/types",
    "link-types:minimal": "cd examples/minimal && npm link @unchainedshop/types",
    "link-types:controlpanel": "cd examples/controlpanel && npm link @unchainedshop/types"
  },
  "devDependencies": {
    "@apollo/client": "^3.6.8",
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@shelf/jest-mongodb": "^2.2.2",
    "@types/jest": "^27.5.2",
    "@types/lodash.clone": "^4.5.7",
    "@types/meteor": "^2.6.3",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.43",
    "@types/simpl-schema": "^1.12.2",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "@unchainedshop/types": "./packages/@unchainedshop/types",
    "apollo-upload-client": "^17.0.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.6",
    "bip32": "^3.0.1",
    "bitcoinjs-lib": "^6.0.1",
    "cross-env": "^7.0.3",
    "dotenv-extended": "^2.9.0",
    "eslint": "^8.17.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "ethers": "^5.6.8",
    "form-data": "^4.0.0",
    "graphql": "^16.5.0",
    "graphql-tag": "^2.12.5",
    "isomorphic-unfetch": "^3.1.0",
    "jest": "^27.0.6",
    "mongodb": "^4.7.0",
    "node-blob": "0.0.2",
    "npm-run-all": "^4.1.5",
    "postfinancecheckout": "^3.0.5",
    "prettier": "^2.7.0",
    "sinon": "^13.0.2",
    "sinon-mongo": "^2.1.0",
    "stripe": "^8.222.0",
    "tiny-secp256k1": "^2.2.1",
    "ts-jest": "^27.1.5",
    "twilio": "^3.77.2",
    "typescript": "^4.7.3"
  }
}
