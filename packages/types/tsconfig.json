{
  "compilerOptions": {
    "declaration": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "isolatedModules": true,
    "lib": ["esnext"],
    "module": "esnext",
    "moduleResolution": "node",
    "outDir": "./lib",
    "preserveWatchOutput": true,
    "skipLibCheck": true,
    "typeRoots": ["node_modules/@types", "node_modules/mongodb"],
    "types": []
  },
  "include": [
    "index.ts",
    "accounts.ts",
    "api.ts",
    "bookmarks.ts",
    "common.ts",
    "countries.ts",
    "currencies.ts",
    "events.ts",
    "files.ts",
    "languages.ts",
    "logs.ts",
    "modules.ts",
    "payments.ts",
    "user.ts",
    "warehousing.ts",
    "worker.ts"
  ]
}
